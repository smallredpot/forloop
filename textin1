#include <iostream>
#include <string>
using namespace std;
int main()
{
    char ch;
    int count = 0;
    cout << "Enter characters; enter # to quit:" << endl;
    cin >> ch;
    while(ch!= '#')
    {
        cout << ch;
        count++;
        cin >> ch;
    }
    cout << endl << count << " charaters read" << endl;
    return 0;
}
